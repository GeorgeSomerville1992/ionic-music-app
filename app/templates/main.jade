ion-side-menus(enable-menu-with-back-views='true')
  ion-side-menu-content
    ion-nav-view(name='viewContent')
  ion-side-menu.side-menu(side='left')
    ion-header-bar.bar-positive(align-title='center')
      h1.title Side Menu
    ion-content.has-header
      .list
        a.item.item-icon-left(menu-close='', ui-sref-active='active', ui-sref='app.profile')
          i.icon.ion-home
          |               Home
        a.item.item-icon-left(menu-close='', ui-sref-active='active', ui-sref='app.settings')
          i.icon.ion-settings
          |               Settings
        a.item.item-icon-left(menu-close='', ui-sref-active='active', ui-sref='app.login')
          i.icon.ion-settings
          |               login
        a.item.item-icon-left(menu-close='', ui-sref-active='active', ng-click='changeStateTest()')
          i.icon.ion-settings
          |               test
        a.item.item-icon-left(menu-close='', ui-sref-active='active', ng-click='logout()')
          i.icon.ion-home
          |               logout
